{
  "plugins": [
    "react",
  ],
  "extends": [
    "eslint:recommended",
    "plugin:react/recommended",
  ],
  "parser": "babel-eslint",
  "parserOptions": {
    "ecmaVersion": 6,
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true
    },
  },
  "env": {
    "mocha": true,
    "node": true,
    "es6": true,
  },
  "rules": {
    "no-console": 0,
     "no-var": 1,
    "prefer-const": 1
  }
}
